<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gistone.mapper.DataSurveyFilesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gistone.entity.DataSurveyFiles">
        <id column="pkid" property="pkid" />
        <result column="index_item_id" property="indexItemId" />
        <result column="identification" property="identification" />
        <result column="path" property="path" />
        <result column="ps01" property="ps01" />
        <result column="ps02" property="ps02" />
        <result column="ps03" property="ps03" />
        <result column="ps04" property="ps04" />
    </resultMap>

	<!-- 批量上传调查点图片 -->
	<insert id="insertBatch" parameterType="java.util.List">
		INSERT INTO data_survey_files (index_item_id, identification, path) 
		VALUES 
		<foreach collection="list" item="item" index="index" separator=",">
			(#{item.indexItemId,jdbcType=INTEGER},#{item.identification,jdbcType=VARCHAR},#{item.path,jdbcType=VARCHAR})
		</foreach>
	</insert>
	
</mapper>
