<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gistone.mapper.DataSurveyJsonMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gistone.entity.DataSurveyJson">
        <id column="pk_id" property="pkId" />
        <result column="fk_survey_id" property="fkSurveyId" />
        <result column="index_item_id" property="indexItemId" />
        <result column="survey_jsonData" property="surveyJsondata" />
        <result column="ps01" property="ps01" />
        <result column="ps02" property="ps02" />
    </resultMap>

	<!-- 保存json -->
	<insert id="insertBatch" parameterType="java.util.List">
	  	INSERT INTO data_survey_json (fk_survey_id,index_item_id,survey_jsonData) VALUES
	  	<foreach collection="list" item="item" index="index" separator=",">
	  		(#{item.fkSurveyId,jdbcType=INTEGER},#{item.indexItemId,jdbcType=INTEGER},#{item.surveyJsondata,jdbcType=VARCHAR})
	  	</foreach>
  	</insert>

</mapper>
